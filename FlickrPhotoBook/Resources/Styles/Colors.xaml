    <Color x:Key="MainColor">#114CA1</Color>
    <Color x:Key="MainColor15">#26114ca1</Color>
    <Color x:Key="MainColorReverse">#FFFFFF</Color>
    
    <Color x:Key="Black10">#1A000000</Color>
    <Color x:Key="Black70">#B3000000</Color>
    <Color x:Key="White25">#40FFFFFF</Color>
    <Color x:Key="White50">#80FFFFFF</Color>
    <Color x:Key="White90">#E6FFFFFF</Color>
    
    <Color x:Key="WhiteTwo">#F2F2F2</Color>
    <Color x:Key="WhiteThree">#EFF0F8</Color>
    <Color x:Key="WhiteFour">#F8F8F8</Color>
    <Color x:Key="WhiteFive">#E0E0E0</Color>
    
    <Color x:Key="SlateGray">#444B51</Color>
    <Color x:Key="Silver">#838396</Color>
    <Color x:Key="SilverSlate">#ADA8B8</Color>
    <Color x:Key="DeepAmethyst">#948EA2</Color>
    
    <Color x:Key="DarkGray">#484451</Color> 
    <Color x:Key="DustyGray">#979797</Color>
    <Color x:Key="DustyGray40">#66979797</Color>
    <Color x:Key="DustyGrayTwo">#989898</Color>
    <Color x:Key="LightGray">#D8D8D8</Color>
    <Color x:Key="LightGrayTwo">#E1E0E3</Color>
    <Color x:Key="LightGrayThree">#EAEAEA</Color>
    <Color x:Key="SilverGray">#6B7F99</Color>
    <Color x:Key="SilverGray30">#4D6B7F99</Color>
